{
  "manifest_version": 3,
  "name": "Domain Switcher",
  "version": "1.5",
  "description": "Простой переключатель доменов для веб-сайтов.",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://localhost:5002/*",
    "https://localhost:5002/*",
    "https://test-gomel-erp.holdingbp.ru/*",
    "https://erp.holdingbp.ru/*"
  ]
}
